{
    "presets": ["es2015", "react","es2016","stage-1"],
    "env": {
      "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }],
        ["import", { "libraryName": "antd", "style": "css" }],
        ["transform-decorators-legacy"] 
        ]
      }
    }
  }